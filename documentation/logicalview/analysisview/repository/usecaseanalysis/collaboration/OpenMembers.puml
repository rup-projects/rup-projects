@startuml

actor user

boundary MenuView
boundary MembersManagementView

control OpenMembersController

entity MembersDao
entity Member

user -right-> MenuView
    note on link
        openMembers()
    end note
MenuView -down-> MembersManagementView
    note on link
        new MembersManagementView()
    end note
MembersManagementView -right-> OpenMembersController
    note on link
        getAllMembers()
    end note

OpenMembersController -right-> MembersDao
    note on link
        getAll()
    end note

MembersManagementView o..> Member
OpenMembersController ..> Member

MembersDao .down.> Member

@enduml
