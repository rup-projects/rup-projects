@startuml

rectangle server {
    package resource
    package service
    package repository
    package entity
    package dto

    artifact ModelMapper

    resource -down-> service
    resource .down.> dto
    service .down..> dto
    service .down.> entity
    service -down-> repository
    service -left-> ModelMapper
    repository .down.> entity
}

rectangle frontend {
    package components
    package models
    package services

    components -down-> models
    components -down-> services
    services -down-> models
}

@enduml
